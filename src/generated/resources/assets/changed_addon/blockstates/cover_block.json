{
  "multipart": [
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "changed_addon:block/cover_block_down",
        "x": 90
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "changed_addon:block/cover_block_up",
        "x": -90
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "changed_addon:block/cover_block_north",
        "y": 180
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "changed_addon:block/cover_block_south"
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "changed_addon:block/cover_block_west",
        "y": 90
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "changed_addon:block/cover_block_east",
        "y": 270
      }
    }
  ]
}